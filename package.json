{
  "name": "website-evidence-collector",
  "version": "1.0.0",
  "description": "automation of website evidence collection for storage and transfer of personal data based on puppeteer",
  "homepage": "https://edps.europa.eu/press-publications/edps-inspection-software_en",
  "repository": {
    "type": "git",
    "url": "https://github.com/EU-EDPS/website-evidence-collector.git"
  },
  "engines": {
    "node": ">=14.0"
  },
  "dependencies": {
    "@cliqz/adblocker-puppeteer": "^1.22.2",
    "fs-extra": "^8.1.0",
    "git-describe": "^4.0.4",
    "github-markdown-css": "^3.0.1",
    "got": "^11.8.5",
    "js-yaml": "^3.14.1",
    "lodash": "^4.17.21",
    "pug": "^3.0.2",
    "puppeteer": "^10.1.0",
    "puppeteer-har": "^1.1.2",
    "stacktrace-js": "^2.0.2",
    "tmp": "^0.1.0",
    "tough-cookie": "^4.0.0",
    "winston": "^3.3.3",
    "yargs": "^17.0.1"
  },
  "devDependencies": {
    "jest": "^27.0.1",
    "pino-pretty": "^3.6.1"
  },
  "scripts": {
    "start": "node ./website-evidence-collector.js https://www.test.com --overwrite --html --json --yaml --screenshots",
    "test": "jest --detectOpenHandles",
    "pretty-print": "node node_modules/.bin/pino-pretty --timestampKey timestamp --messageKey type --ignore stack,raw,origin"
  },  
  "bin": {
    "website-evidence-collector": "./bin/website-evidence-collector.js",
    "website-evidence-reporter": "./bin/website-evidence-reporter.js"
  },
  "keywords": [
    "privacy",
    "tracking",
    "website",
    "data",
    "protection",
    "personal",
    "data",
    "GDPR",
    "puppeteer"
  ],
  "author": "Robert Riemann <robert.riemann@edps.europa.eu>",
  "license": "EUPL-1.2"
}
